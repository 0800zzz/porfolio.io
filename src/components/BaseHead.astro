---
import "@/styles/global.css";
import { SITE_TITLE } from "@/consts";
interface Props { title?: string; description?: string; image?: string; }
const { title = SITE_TITLE, description = "Pentesting • Purple Team • Writeups" } = Astro.props;
---
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<title>{title}</title>
<meta name="description" content={description} />
<link rel="icon" type="image/svg+xml" href="https://emoji-route.deno.dev/svg/👩‍💻" />
